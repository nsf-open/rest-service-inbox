<?xml version="1.0" encoding="UTF-8"?>
<wls:weblogic-web-app xmlns:wls="http://www.bea.com/ns/weblogic/weblogic-web-app">
	<wls:context-root>inboxservice</wls:context-root>

	<container-descriptor>
		<prefer-web-inf-classes>true</prefer-web-inf-classes>
	</container-descriptor>

	<!-- Security Constraints for Service to Service to Basic Auth. Role svcrole -->
	<wls:security-role-assignment>
		<wls:role-name>svcrole</wls:role-name>
		<wls:principal-name>SvcClientGroup</wls:principal-name>
	</wls:security-role-assignment>
</wls:weblogic-web-app>